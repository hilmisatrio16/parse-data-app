<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.ParseDataApp"
        tools:targetApi="31">
        <activity
            android:name=".DataParcelableActivity"
            android:exported="false" />
        <activity
            android:name=".DataSerializableActivity"
            android:exported="false" />
        <activity
            android:name=".DataBundleActivity"
            android:exported="false" />
        <activity
            android:name=".ParcelableActivity"
            android:exported="false" />
        <activity
            android:name=".SerializableActivity"
            android:exported="false">
        </activity>
        <activity
            android:name=".BundleActivity"
            android:exported="false" />
        <activity
            android:name=".HomeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">
        </activity>
    </application>

</manifest>